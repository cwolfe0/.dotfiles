{
   "author": "Josh Haimson and Dan Elitzer",
   "background": {
      "scripts": [ "request.js" ]
   },
   "description": "Redirects any www.amazon.com page to smile.amazon.com",
   "homepage_url": "http://www.smilealways.io",
   "icons": {
      "128": "SA-128x128.png",
      "16": "SA-16x16.png",
      "48": "SA-48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Smile Always",
   "permissions": [ "webRequest", "webRequestBlocking", "http://www.amazon.com/*", "https://www.amazon.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.91"
}
